@model IEnumerable<VeterinarskaStanica.Models.Administrator>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KorisnickiNalog.Username)
        </th>
        <th>
            Neaktivan
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Prezime)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.KorisnickiNalog.Username)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsDeleted)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prezime)
            </td>

            <td>
                <a href="#"
                   class="btn btn-default"
                   data-ajax="true"
                   data-ajax-url="/ModulAdmin/Administrator/Edit?id=@item.Id"
                   data-ajax-update="#ajaxpozivKorisnici">Edit</a>
            </td>
        </tr>
    }

</table>